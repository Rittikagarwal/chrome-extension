{
    "manifest_version": 2,
    "name": "Test Extension",
    "version": "1.0",
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "tabs",
        "webNavigation",
        "activeTab",
        "storage",
        "webRequest",
        "webRequestBlocking",
        "notifications"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    }
}
